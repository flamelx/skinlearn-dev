<launch>

   <rosparam file="$(find ice_robot_controllers)/config/pr2_controller.yaml"
             command="load" />
  
   <rosparam file="$(find ice_robot_controllers)/config/l_ft_calib_data.yaml"
             command="load" />

  <node name="unspawn_arms"
        pkg="pr2_controller_manager"
        type="unspawner"
        args="l_arm_controller r_arm_controller" />

   <node pkg="pr2_controller_manager"
         type="spawner"
         args="pr2_adaptNeuroController"
         name="pr2_adaptNeuroController_spawner" />

</launch>
